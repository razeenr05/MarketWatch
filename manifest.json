{
    "manifest_version": 3,
    "name": "Stock Ticker",
    "version": "1.0",
    "description": "Displays the current price of a user-selected stock.",
    "permissions": ["storage", "alarms"],
    "host_permissions": ["https://finnhub.io/*"],
    "background": {
      "service_worker": "background.js"},
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "web_accessible_resources": [
      {
        "resources": ["config.js"],
        "matches": ["<all_urls>"]
      }
    ] 
  }
  